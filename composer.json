{
  "name": "danyi/custom-enum",
  "authors": [
    {
      "name": "Dániel SZÖLLŐSI",
      "role": "Owner",
      "homepage": "https://github.com/D4nyi"
    }
  ],
  "type": "library",
  "description": "A custom enum implementation for PHP.",
  "keywords": [
    "enum",
    "custom enum"
  ],
  "license": "MIT",
  "minimum-stability": "stable",
  "require": {
    "php": "^7.4"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.4.0",
    "mockery/mockery": "^1.4.0"
  },
  "autoload": {
    "psr-4": {
      "CustomEnum\\": "src/"
    },
    "psr-0": {
      "CustomEnum\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-0": {
      "Tests\\": "tests/",
      "Example\\": "examples/"
    },
    "psr-4": {
      "Tests\\": "tests/",
      "Example\\": "examples/"
    }
  },
  "scripts": {
    "test": "phpunit --configuration phpunit.xml tests --teamcity --cache-result-file=.phpunit.result.cache"
  }
}
